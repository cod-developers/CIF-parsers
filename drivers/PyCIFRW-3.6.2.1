#!/bin/bash

TMP_DIR="${TMPDIR}"
BASENAME="$(basename $0)"

test -z "${TMP_DIR}" && TMP_DIR="."
TMP_FILE="${TMP_DIR}/tmp-${BASENAME}-$$"

export PYTHONPATH=~/src/PyCifRW-3.6.2.1/dist-packages/lib/python2.7/site-packages

pycifrw-parse "$@" > ${TMP_FILE} 2>&1

test -s ${TMP_FILE} && echo ERROR

rm "${TMP_FILE}"
